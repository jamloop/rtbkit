{
  "endpoints": {
    "logger": {
       "protocol": "zmq",
       "alias": "logs",
       "type": "publisher",
       "port": 16000
    },
    "router.agents": {
      "serviceName": "agents",
      "alias": "router",
      "protocol": "zmq",
      "type": "bus",
      "port": 17000
    },
    "augmentationLoop": {
      "serviceName": "augmentors",
      "alias": "augmentors",
      "protocol": "zmq",
      "type": "bus",
      "port": 18000
    },
    "agentConfiguration.agents": {
        "serviceName": "agents",
        "protocol": "zmq",
        "type": "bus",
        "port": 19000
    },
    "pal.events": {
      "serviceName": "events",
      "alias": "postAuctionLoop",
      "protocol": "zmq",
      "type": "bus",
      "port": 20000
    },
    "pal.agents": {
      "serviceName": "agents",
      "alias": "postAuctionLoopAgents",
      "protocol": "zmq",
      "type": "bus",
      "port": 21000
    },
    "banker.rest": {
      "protocol": "rest",
      "ports": {
        "http": 9876,
        "zeromq": 22000
      }
    },
    "agentConfiguration.rest": {
      "protocol": "rest",
      "ports": {
        "http": 9986,
        "zeromq": 23000
      }
    },
    "monitor.rest": {
      "protocol": "rest",
      "ports": {
        "http": 9987,
        "zeromq": 24000
      }
    },
    "agentConfiguration.listeners": {
      "serviceName": "listen",
      "alias": "configuration",
      "protocol": "zmq",
      "type": "bus",
      "port": 26000
    }
  },
  
  "services": {
    "rtbAgentConfiguration": {
      "useast1b.rtb1.acs": {
           "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
           "bind": [ "agentConfiguration.rest", "agentConfiguration.listeners", "agentConfiguration.agents" ]
      }
    },
    "rtbRequestRouter": {
        "useast1b.rtb1.router1": {
           "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
            "bind": [ "logger", "router.agents" ]
         },
        "useast1b.rtb2.router1": {
           "hostname": "ec2-52-200-122-54.compute-1.amazonaws.com",
            "bind": [ "logger", "router.agents" ]
         }
    },
    "rtbRouterAugmentation": {
        "useast1b.rtb1.router1": {
            "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
            "bind": [ "augmentationLoop" ]
      },
      "useast1b.rtb2.router1": {
            "hostname": "ec2-52-200-122-54.compute-1.amazonaws.com",
            "bind": [ "augmentationLoop" ]
      }
    },
    "rtbPostAuctionService": {
       "useast1b.rtb1.pal": {
            "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
            "bind": [ "logger:$+1", "pal.agents", "pal.events" ],
            "shard": 0
        }
    },
    "adServer": {
      "useast1b.rtb1.events": {
           "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
           "bind": [ "logger:$+2" ]
       }
    },
    "monitor": {
      "useast1b.rtb1.monitor": {
           "hostname": "ec2-52-6-205-74.compute-1.amazonaws.com",
           "bind": [ "monitor.rest" ]
       }
    }
  }
}
